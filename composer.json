{
    "name": "coderockr/silex-skel",
    "description": "Silex Skel",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "silex/silex": "v1.3.4",
        "symfony/yaml": "~2.4",
        "monolog/monolog": "^1.17",
        "doctrine/dbal": "^2.5",
        "deralex/yaml-config-service-provider": "^1.0",
        "dflydev/doctrine-orm-service-provider": "1.0.*@dev",
        "doctrine/migrations": "^1.1",
        "respect/validation": "^1.0",
        "symfony/console": "2.5"
    },
    "require-dev": {
        "symfony/browser-kit": "*",
        "phpunit/phpunit": "4.6.*",
        "squizlabs/php_codesniffer": "^2.0@dev",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload": {
        "psr-0": {
            "Pastelaria\\": "src/"
        }
    },
    "config": {
        "process-timeout": 9000,
        "secure-http":false
    },
    "repositories": [{
        "type": "composer",
        "url": "http://resource.ftdata.com.br:8086"
    }]
}
